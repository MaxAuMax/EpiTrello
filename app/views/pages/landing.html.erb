<style>
  /* Landing Page Styles */
  html {
    scroll-behavior: smooth;
  }

  .landing-page {
    min-height: 100vh;
    background: white;
    font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Oxygen, Ubuntu, Cantarell, sans-serif;
  }

  .landing-page * {
    box-sizing: border-box;
  }

  /* Navigation */
  .landing-nav {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 50;
    background: rgba(255, 255, 255, 0.8);
    backdrop-filter: blur(12px);
    border-bottom: 1px solid #f1f5f9;
  }

  .landing-nav .container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 1rem;
    height: 64px;
    display: flex;
    align-items: center;
    justify-content: space-between;
  }

  .landing-logo {
    display: flex;
    align-items: center;
    gap: 0.5rem;
    text-decoration: none;
    user-select: none;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
  }

  .landing-logo-icon {
    width: 32px;
    height: 32px;
    background: linear-gradient(to bottom right, #f97316, #ec4899);
    border-radius: 8px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
  }

  .landing-logo-text {
    font-weight: 700;
    font-size: 1.25rem;
    color: #0f172a;
    letter-spacing: -0.025em;
  }

  .landing-nav-links {
    display: none;
    align-items: center;
    gap: 2rem;
    font-size: 0.875rem;
    font-weight: 500;
    color: #64748b;
  }

  @media (min-width: 768px) {
    .landing-nav-links {
      display: flex;
    }
  }

  .landing-nav-links a {
    color: #64748b;
    text-decoration: none;
    transition: color 0.2s;
  }

  .landing-nav-links a:hover {
    color: #0f172a;
  }

  .landing-nav-actions {
    display: flex;
    align-items: center;
    gap: 1rem;
  }

  .landing-btn-login {
    display: none;
    font-size: 0.875rem;
    font-weight: 500;
    color: #64748b;
    text-decoration: none;
  }

  @media (min-width: 768px) {
    .landing-btn-login {
      display: block;
    }
  }

  .landing-btn-login:hover {
    color: #0f172a;
  }

  .landing-btn-signup {
    padding: 0.5rem 1rem;
    background: #0f172a;
    color: white;
    border-radius: 9999px;
    font-size: 0.875rem;
    font-weight: 500;
    text-decoration: none;
    transition: background 0.2s;
    display: inline-block;
  }

  .landing-btn-signup:hover {
    background: #1e293b;
  }

  /* Main Content */
  .landing-main {
    padding-top: 64px;
  }

  /* Container */
  .container {
    max-width: 100%;
    width: 100%;
    margin: 0 auto;
    padding: 0 2rem;
  }

  /* Social Proof */
  .social-proof {
    border-top: 1px solid #f1f5f9;
    border-bottom: 1px solid #f1f5f9;
    background: white;
    padding: 2.5rem 0;
  }

  .social-proof-title {
    text-align: center;
    font-size: 0.875rem;
    font-weight: 600;
    color: #cbd5e1;
    text-transform: uppercase;
    letter-spacing: 0.05em;
    margin-bottom: 1.5rem;
  }

  .social-proof-logos {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
    gap: 2rem;
  }

  .social-proof-logo {
    height: 48px;
    width: auto;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .social-proof-logo:empty {
    height: 32px;
    width: 96px;
    filter: grayscale(1) opacity(0.5);
    background: #1e293b;
    border-radius: 4px;
  }

  .social-proof-logo svg {
    height: 100%;
    width: auto;
  }

  /* Footer */
  .landing-footer {
    background: #f8fafc;
    border-top: 1px solid #e2e8f0;
    padding: 2rem 0;
  }

  .footer-bottom {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    gap: 1rem;
    padding: 0 2rem;
  }

  .footer-logo {
    display: flex;
    align-items: center;
    gap: 0.5rem;
  }

  @media (min-width: 768px) {
    .footer-logo {
      margin-bottom: 0;
    }
  }

  .footer-logo-icon {
    width: 24px;
    height: 24px;
    background: linear-gradient(to bottom right, #f97316, #ec4899);
    border-radius: 4px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
  }

  .footer-logo-text {
    font-weight: 700;
    color: #0f172a;
  }

  .footer-copyright {
    font-size: 0.875rem;
    color: #94a3b8;
  }
</style>

